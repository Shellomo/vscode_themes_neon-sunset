{
  "name": "Neon Sunset",
  "type": "dark",
  "colors": {
    "editor.background": "#1a1b26",
    "editor.foreground": "#d4d4d4",
    "activityBarBadge.background": "#ff7edb",
    "sideBarTitle.foreground": "#bbbbbb",
    "titleBar.activeBackground": "#1a1b26",
    "statusBar.background": "#1a1b26",
    "statusBar.noFolderBackground": "#1a1b26",
    "statusBar.debuggingBackground": "#ff7edb",
    "list.activeSelectionBackground": "#313244",
    "list.hoverBackground": "#292e42",
    "editor.lineHighlightBackground": "#28293d",
    "editor.selectionBackground": "#515c7e",
    "editorCursor.foreground": "#ff7edb",
    "terminal.background": "#1a1b26",
    "terminal.foreground": "#d4d4d4"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#636b6f"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted.double.json"],
      "settings": {
        "foreground": "#7ee787"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ff7edb"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#82aaff"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other.object"],
      "settings": {
        "foreground": "#ffd700"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.type", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#ff9e64"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#ff9e64"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant.language", "constant.character"],
      "settings": {
        "foreground": "#bb9af7"
      }
    },
    {
      "name": "Tags",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#7aa2f7"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ff7edb"
      }
    }
  ]
}